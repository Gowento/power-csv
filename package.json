{
  "name": "power-csv",
  "version": "1.3.1",
  "description": "Helpers to deal with CSV format",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test --cache",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "npm run test -s",
    "test": "NODE_ENV=test ava",
    "coverage": "nyc npm test && nyc report --reporter=html",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "ava": {
    "failFast": false,
    "verbose": false,
    "require": [
      "babel-core/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gowento/power-csv.git"
  },
  "author": "Loris Guignard <loris.guignard@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gowento/power-csv#readme",
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "6.7.2",
    "coveralls": "2.11.9",
    "dependency-check": "2.5.1",
    "doctoc": "1.0.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-plugin-ava": "2.0.1",
    "express": "4.13.4",
    "nyc": "6.4.3",
    "rimraf": "2.5.2",
    "supertest": "1.2.0",
    "supertest-as-promised": "3.1.0",
    "watch": "0.17.1"
  },
  "dependencies": {
    "babel-eslint": "6.0.2",
    "babyparse": "0.4.5",
    "eslint": "2.7.0",
    "eslint-plugin-ava": "2.2.1",
    "flat": "2.0.0",
    "lodash": "4.11.1",
    "supertest-as-promised": "3.1.0"
  }
}
